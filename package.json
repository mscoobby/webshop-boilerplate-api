{
  "name": "webshop-boilerplate-api",
  "version": "1.0.0",
  "description": "A boilerplate for Node.js web applications",
  "repository": {
    "type": "git",
    "url": "https://github.com/mscoobby/webshop-boilerplate-api"
  },
  "author": "Georgi Stefanov",
  "license": "MIT",
  "scripts": {
    "start": "nodemon app.js",
    "test": "mocha --reporter spec",
    "postinstall": "npm rebuild node-sass",
    "eslint": "eslint **/*.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "chalk": "^2.0.1",
    "compression": "^1.7.0",
    "config": "^1.26.1",
    "connect-mongo": "^1.3.2",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "express-flash": "^0.0.2",
    "express-session": "^1.14.2",
    "express-status-monitor": "^1.0.0",
    "express-validator": "^5.0.0",
    "jsonwebtoken": "^8.0.0",
    "lusca": "^1.4.1",
    "mongoose": "^4.6.6",
    "morgan": "^1.7.0",
    "node-sass-middleware": "^0.11.0",
    "nodemailer": "^4.0.1",
    "passport": "0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-instagram": "^1.0.0",
    "passport-instagram-token": "^2.3.0",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "passport-twitter-token": "^1.3.0",
    "paypal-rest-sdk": "^1.7.0",
    "pug": "^2.0.0-beta6",
    "sinon": "^2.4.0",
    "snyk": "^1.88.0",
    "stripe": "^4.12.0",
    "twit": "^2.2.5"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-chai-friendly": "^0.3.6",
    "eslint-plugin-import": "^2.1.0",
    "mocha": "^3.1.2",
    "sinon": "^2.0.0",
    "sinon-mongoose": "^2.0.2",
    "supertest": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "no-unused-expressions": 0,
      "chai-friendly/no-unused-expressions": 2
    },
    "env": {
      "jasmine": true,
      "mocha": true,
      "node": true
    },
    "plugins": [
      "chai-friendly"
    ]
  },
  "snyk": true
}
